
<div class="main users-new">
  <div class="container">
    <h1 class="form-heading">新規登録</h1>

    <div class="form">
      <div class="form-body">
        <%= form_tag("/users/create") do %>
          <p>ユーザーネーム</p>
            <input name="name" >
          <p>メールアドレス</p>
            <input name="email" >
            <input type="submit" name="" value="登録">
        <% end %>
      </div>
    </div>
  </div>
</div>
